{
  "name": "loan-collection-intelligence",
  "version": "1.0.0",
  "description": "Loan Collection Conversation Intelligence Platform",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "nodemon server.js",
    "seed": "node db/seed.js",
    "reseed": "node -e \"const db=require('./db/setup');db.initialize().then(()=>db.sequelize.sync({force:true})).then(()=>require('./db/seed').seed()).then(()=>process.exit(0))\"",
    "health": "node -e \"fetch('http://localhost:3000/health/ready').then(r=>r.json()).then(console.log).catch(console.error)\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "form-data": "^4.0.5",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "sequelize": "^6.37.3",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.14"
  }
}
